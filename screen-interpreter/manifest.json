{
  "manifest_version": 3,
  "name": "Screen Recorder",
  "version": "1.0",
  "description": "Una extensi√≥n para grabar la pantalla",
  "permissions": [
    "desktopCapture",
    "storage"
  ],
  "host_permissions": [
    "http://127.0.0.1:5001/*"
  ],
  "action": {
    "default_icon": {
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
} 